{
    "name": "bitworking/mimeparse",
    "description": "Basic functions for handling mime-types, forked from Joe Gregorio's mimeparse library <http://code.google.com/p/mimeparse/>.",
    "type": "library",
    "keywords": ["http", "accept", "mime"],
    "license": "MIT",
    "authors": [
        {
            "name": "Joe Gregorio",
            "homepage": "http://bitworking.org"
        },
        {
            "name": "Andrew \"Venom\" K."
        },
        {
            "name": "Ben Ramsey",
            "homepage": "http://benramsey.com"
        }
    ],
    "support": {
        "issues": "https://github.com/conneg/mimeparse-php/issues",
        "source": "https://github.com/conneg/mimeparse-php"
    },
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "squizlabs/php_codesniffer": "~2.2",
        "jakub-onderka/php-parallel-lint": "0.8.*",
        "satooshi/php-coveralls": "0.6.*"
    },
    "autoload": {
        "psr-4": { "Bitworking\\": "src/Bitworking/" }
    }
}
